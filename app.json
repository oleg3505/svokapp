{
  "expo": {
    "name": "svokapp",
    "slug": "svokapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kreolko.svokapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kreolko.svokapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#232323",
          "image": "./assets/images/splash-icon.png"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6873a4e0-8e16-40a3-9df7-d0f8681517c1"
      }
    }
  }
}
